apiVersion: projectcontour.io/v1
kind: HTTPProxy
metadata:
  name: sonarqube-httpproxy
  namespace: sonarqube
spec:
  virtualhost:
    fqdn: sonarqube.{{ sonarqube.domain }}
    tls:
      secretName: sonarqube-ingress-tls
  routes:
    - conditions:
        - prefix: /
      services:
        - name: sonarqube-sonarqube
          port: 9000