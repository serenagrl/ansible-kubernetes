apiVersion: projectcontour.io/v1
kind: HTTPProxy
metadata:
  name: gitlab-minio-console-httpproxy
  namespace: gitlab
spec:
  virtualhost:
    fqdn: gitlab-console.{{ gitlab_minio.domain }}
    tls:
      secretName: gitlab-minio-ingress-tls
  routes:
    - conditions:
        - prefix: /
      services:
        - name: gitlab-minio-console
          port: 9090
