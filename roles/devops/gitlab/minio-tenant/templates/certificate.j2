apiVersion: cert-manager.io/v1
kind: Certificate
metadata:
  name: gitlab-minio-ingress-cert
  namespace: gitlab
spec:
  dnsNames:
    - gitlab.{{ gitlab_minio.domain }}
    - gitlab-console.{{ gitlab_minio.domain }}
  issuerRef:
    kind: ClusterIssuer
    name: {{ fact_cluster_issuer_name }}
  secretName: gitlab-minio-ingress-tls