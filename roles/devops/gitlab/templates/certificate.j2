apiVersion: cert-manager.io/v1
kind: Certificate
metadata:
  name: gitlab-ingress-cert
  namespace: gitlab
spec:
  dnsNames:
    - gitlab.{{ domain }}
    - kas.{{ domain }}
    - minio.{{ domain }}
    - registry.{{ domain }}
  issuerRef:
    kind: ClusterIssuer
    name: default-cluster-issuer
  secretName: gitlab-ingress-tls