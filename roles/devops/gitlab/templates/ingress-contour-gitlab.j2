apiVersion: projectcontour.io/v1
kind: HTTPProxy
metadata:
  name: gitlab-httpproxy
  namespace: gitlab
spec:
  virtualhost:
    fqdn: gitlab.{{ domain }}
    tls:
      secretName: gitlab-ingress-tls
  routes:
    - conditions:
        - prefix: /
      services:
        - name: gitlab-webservice-default
          port: 8181
