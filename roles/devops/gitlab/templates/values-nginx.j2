fullnameOverride: gitlab
global:
  hosts:
    domain: {{ domain }}
  ingress:
{% if has_issuer %}
    tls:
      secretName: gitlab-ingress-tls
{% endif %}
    class: {{ ingress_name }}
    configureCertmanager: false

gitlab-runner:
  install: false

certmanager:
  install: false

nginx-ingress:
  enabled: false

prometheus:
  install: false

registry:
  enabled: false
